{
  "name": "y-official-extension",
  "displayName": "Y Official Extension",
  "description": "Extension del lenguaje Y para Visual Studio Code.",
  "version": "1.0.5",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "y",
        "aliases": ["Y", "y"],
        "extensions": [".y"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "y",
        "scopeName": "source.y",
        "path": "./syntaxes/y.tmLanguage.json"
      }
    ],
    "icons": [
      {
        "file": "YLOGO.png",
        "extension": ".y"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Danco54/Y-Official-Extension"
  },
  "publisher": "Danco540"
}
